{
  "name": "qbittorrent-exporter",
  "version": "1.0.2",
  "description": "Export your qbittorrent data to prometheus",
  "main": "src/init.go",
  "scripts": {
    "build" : "go build -o ./qbittorrent-exporter.out ./src && ./qbittorrent-exporter.out -e",
    "build:env" : "go build -o ./qbittorrent-exporter.out ./src && ./qbittorrent-exporter.out -e",
    "dev" : "go run ./src",
    "dev:env" : "go run ./src -e",
    "format" : "go fmt ./src",
    "test": "go test -v ./src/tests",
    "update": "go get -u ./src && go mod tidy"
  },
  "keywords": [
    "golang",
    "qbittorrent",
    "prometheus",
    "export",
    "grafana",
    "dashboard",
    "metrics"
  ],
  "author": "martabal",
  "license": "MIT"
}
